# Production environment - Use relative paths for ngrok/cloudflare compatibility
# Empty VITE_API_URL means frontend will use relative paths
# Nginx inside container will proxy /services/* to backend

# Gateway backend - EMPTY = use relative path (recommended for public access)
VITE_API_URL=

# AI Service - EMPTY = use relative path
VITE_AI_SERVICE_URL=

VITE_DEMO_MODE=false
VITE_DEBUG=false
